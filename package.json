{
  "name": "react-redux-universal-fhir",
  "version": "0.0.2",
  "description": "A React/Redux/Universal/MongoDB FHIR REST Server",
  "main": "bin/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "make test",
    "pre-start": "node index.js",
    "download": "bin/download-fhir-examples.sh",
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "npm run clean && node devServer.js",
    "api": "node ./api/server.js",
    "lint": "eslint src",
    "watch-client": "better-npm-run watch-client",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\""
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "axios": "^0.7.0",
    "babel": "^5.8.23",
    "babel-plugin-typecheck": "^2.0.0",
    "babel-polyfill": "^6.3.14",
    "body-parser": "^1.14.1",
    "classnames": "^2.1.5",
    "compression": "^1.6.0",
    "express": "~4.10.2",
    "express-session": "^1.11.3",
    "history": "^1.12.5",
    "http-proxy": "^1.12.0",
    "install": "^0.4.1",
    "invariant": "^2.2.0",
    "lodash": "^3.10.1",
    "mongoose": "^4.2.10",
    "multireducer": "^1.0.2",
    "ngraph.graph": "0.0.12",
    "ngraph.shremlin": "0.0.2",
    "npm": "^3.5.2",
    "object-path": "~0.6.0",
    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "query-string": "^3.0.0",
    "react": "^0.14.0",
    "react-document-meta": "^2.0.0",
    "react-dom": "^0.14.0",
    "react-redux": "^3.1.0",
    "react-router": "1.0.0-rc3",
    "redux": "^3.0.2",
    "redux-form": "^2.1.0",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "scroll-behavior": "^0.3.0",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "shallowequal": "^0.2.2",
    "superagent": "^1.6.1",
    "warning": "^2.1.0",
    "webpack-isomorphic-tools": "^2.2.23"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.4.7",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.0.5",
    "concurrently": "^1.0.0",
    "css-loader": "^0.19.0",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "node-sass": "^3.3.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
